              </columns>
            </TableView>
            <TextField fx:id="association_search" alignment="TOP_LEFT" layoutX="173.0" layoutY="51.0" onKeyTyped="#associationSearch" prefHeight="26.0" prefWidth="172.0" promptText="Search" styleClass="search" stylesheets="@dashboardDesign.css" />
            <FontAwesomeIconView glyphName="SEARCH" layoutX="179.0" layoutY="70.0" size="19.0" />
