            <Label layoutX="14.0" layoutY="110.0" text="Nom " />
            <Label layoutX="14.0" layoutY="197.0" text="Description " />
            <TextField fx:id="association_nom" layoutX="14.0" layoutY="144.0" promptText="Nom" styleClass="label-info" stylesheets="@dashboardDesign.css" />
