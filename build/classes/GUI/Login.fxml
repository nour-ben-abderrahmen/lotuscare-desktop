<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="786.0" prefWidth="1366.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.LoginController">
   <children>
      <TextField fx:id="emailInput" layoutX="504.0" layoutY="236.0" prefHeight="43.0" prefWidth="265.0" promptText="Email">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <TextField fx:id="passwordInput" layoutX="504.0" layoutY="332.0" prefHeight="43.0" prefWidth="265.0" promptText="Password">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Button layoutX="584.0" layoutY="430.0" mnemonicParsing="false" onAction="#loginAction" text="login">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="683.0" layoutY="437.0" mnemonicParsing="false" onAction="#redirectToSignup" text="singup" />
   </children>
</AnchorPane>
